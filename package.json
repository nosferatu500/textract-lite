{
  "name": "@nosferatu500/textract-lite",
  "version": "3.0.4",
  "homepage": "https://github.com/nosferatu500/textract-lite",
  "description": "Extracting text from files of various type including html, pdf, doc, docx, xls, xlsx, csv, pptx, png, jpg, gif, rtf, text/*, and various open office.",
  "contributors": [
    {
      "name": "David Bashford",
      "email": "dbashford@hotmail.com"
    },
    {
      "name": "Vitalii Shvetsov",
      "email": "vitalya199529@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nosferatu500/textract-lite"
  },
  "keywords": [
    "textract-lite",
    "textract",
    "extract",
    "docx",
    "text"
  ],
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "bracketSameLine": false,
    "tabWidth": 4,
    "printWidth": 120
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.1",
    "html-entities": "2.3.2",
    "iconv-lite": "0.6.3",
    "jschardet": "3.0.0",
    "meow": "^9.0.0",
    "mime": "3.0.0",
    "xpath": "0.0.32",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha",
    "prettier": "prettier --write '{lib,test}/**/*.js'",
    "lint": "yarn prettier && node_modules/.bin/eslint -c .eslintrc.json lib --fix"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "textract": "./bin/textract"
  },
  "main": "./lib/index"
}
